        "drizzle-orm": "^0.44.2",
        "fastify": "^5.4.0",
        "fastify-type-provider-zod": "^4.0.2",
        "postgres": "^3.4.7",
        "uuidv7": "^1.0.2",
        "zod": "^3.25.67"
