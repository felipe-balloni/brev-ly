{
    "name": "server",
    "version": "1.0.0",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "tsx watch --env-file .env src/server.ts",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:migrate:test": "dotenv -e .env.testing drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "clean": "rm -rf dist",
        "build": "tsup-node",
        "start": "node dist/server.js",
        "test": "dotenv -e .env.testing -- vitest run",
        "test:watch": "dotenv -e .env.testing -- vitest",
        "test:coverage": "dotenv -e .env.testing -- vitest run --coverage",
        "pretest:watch": "pnpm run db:migrate:test",
        "pretest": "pnpm run db:migrate:test"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@biomejs/biome": "^2.0.6",
        "@faker-js/faker": "^9.9.0",
        "@types/node": "^24.0.10",
        "@types/pg": "^8.15.4",
        "dotenv-cli": "^8.0.0",
        "drizzle-kit": "^0.31.4",
        "tsup": "^8.5.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.842.0",
        "@aws-sdk/lib-storage": "^3.842.0",
        "@fastify/cors": "^11.0.1",
        "@fastify/swagger": "^9.5.1",
